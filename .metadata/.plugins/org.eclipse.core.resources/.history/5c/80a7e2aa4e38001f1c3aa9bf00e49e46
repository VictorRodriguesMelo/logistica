package com.logistica;

import org.junit.jupiter.api.Test;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.test.context.SpringBootTest;

import com.logistica.controller.FreteController;
import com.logistica.entity.Frete;
import com.logistica.service.FreteService;

@SpringBootTest
class LogisticaApplicationTests {

	@Test
	void contextLoads() {
		initFrete();
	}
	
	public void initFrete() {
		
		@Autowired
	    private final FreteService freteService;
		
		FreteController fretecontroller = new FreteController(null);
		Frete frete = new Frete(1, "caminhonete", 1000);
		Frete freteUm = new Frete(2, "furgao", 50);
		Frete fretea = new Frete(3, "caminhao", 10);
		
		fretecontroller.registerFrete(frete);
		fretecontroller.registerFrete(freteUm);
		fretecontroller.registerFrete(fretea);
	}

}
